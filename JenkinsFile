pipeline {
  agent{
  docker {
      image 'mcr.microsoft.com/playwright:v1.34.3-jammy'
      label 'my-docker-agent'
    }
  }
  stages {
    stage('install playwright') {
      steps {
        sh '''
          npm i -D @playwright/test
          npx playwright install
        '''
      }
    }
    stage('test') {
      steps {
        sh '''
          npx playwright test --list
          npx playwright test
        '''
      }
    }
  }
}